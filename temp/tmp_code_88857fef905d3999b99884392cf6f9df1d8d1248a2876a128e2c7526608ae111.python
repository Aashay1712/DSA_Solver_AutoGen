def add_numbers(a, b):
  """
  This function takes two numbers as input and returns their sum.
  """
  return a + b

# Test cases
test_cases = [
    (5, 3),        # Positive integers
    (-10, 7),      # Negative and positive integer
    (2.5, 3.5),    # Floating-point numbers
    (0, 0),        # Zeros
    (-5, -3)       # Negative integers
]

for num1, num2 in test_cases:
    result = add_numbers(num1, num2)
    print(f"Adding {num1} and {num2} results in: {result}")
